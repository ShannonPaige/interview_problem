<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <h1><%= @users.count %> Users</h1>
    </div>
  </div>
  <div class="row"></div>
  <% @users.each do |user| %>
    <div class="col-sm-12">
      <h2>User <%= user.id %></h2>
      <p><strong>First Name:</strong> <%= user.first_name %></p>
      <p><strong>Last Name:</strong> <%= user.last_name %></p>
      <p><strong>Email:</strong> <%= user.email %></p>
    </div>
  <% end %>
</div>
